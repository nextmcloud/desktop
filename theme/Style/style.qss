
QToolBar { background: qproperty-backgroundColor; margin: 0; padding: 0; border: none; border-bottom: 1px solid qproperty-dark; spacing: 0; }
QToolBar QToolButton { background: qproperty-backgroundColor; border: none; border-bottom: 1px solid qproperty-dark; margin: 0; padding: 5px; }
